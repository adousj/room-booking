
<div class="row">

    <% if @apps %>
      <table class="table table-striped table-hover">
        <caption><h2>讨论室申请</h2></caption>
        <thead>
          <tr>
            <th>id</th>
            <th>姓名</th>
            <th>房间号</th>
            <th>时间</th>
            <th>邮箱</th>
            <th>手机</th>
            <th>附言</th>
            <th>申请时间</th>
            <th>状态</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <%= partial 'apps/app', :collection => @apps %>
        </tbody>
      </table>
    <% else %>
      <p>您目前没有任何申请 :)</p>
    <% end %>

</div>
